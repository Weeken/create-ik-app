{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "shippedProposals": true,
      "corejs": { "version": 3, "proposals": true },
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }]
  ],
  "plugins": [
    ["@babel/plugin-syntax-dynamic-import"],
    [
      "@babel/transform-runtime", {
        "corejs": 3
      }
    ]
  ]
}